# converts hours to minutes so sorting is easier

import re

string = "typically 50 min - 1 h 50 min"

if " h " in string:
    original = re.findall("(\w\sh\s\w*\s)", string)
    for i in original:
        split = i.split("h")
        hrs = int(split[0])
        mins = int(split[1])
        convert_hrs = hrs*60
        total_mins = str(convert_hrs+mins)+" "
